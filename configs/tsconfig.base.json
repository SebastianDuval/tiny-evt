{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "sourceMap": false,
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "allowJs": false,
    "noEmit": true,
  }
}