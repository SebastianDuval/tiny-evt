{
  "name": "tiny-evt",
  "description": "a tiny codebase for building electron-vue-typescript application with vite",
  "version": "0.1.0",
  "author": "Robocat<neatfx@gmail.com>",
  "main": "./build/main.js",
  "scripts": {
    "dev": "esbuild --platform=node --bundle --external:vite scripts/dev-runner.ts --outfile=build/dev-runner.js && env-cmd node build/dev-runner.js",
    "dist": "esbuild scripts/app-builder.ts --outfile=build/app-builder.js && node build/app-builder.js"
  },
  "dependencies": {
    "vue-router": "^4.0.0-alpha.12"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-beta.14",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.0",
    "env-cmd": "^10.1.0",
    "vite": "^0.16.7"
  }
}
